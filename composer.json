{
  "name": "ppi/framework",
  "description": "PPI Framework - The PHP Interoperability Framework!",
  "homepage": "http://www.ppi.io",
  "license": "MIT",
  "keywords": ["framework"],
  "type": "library",
  "authors": [
    {
      "name": "Paul Dragoonis",
      "email": "paul@ppi.io"
    },
    {
      "name": "Vítor Brandão",
      "email": "vitor@ppi.io"
    },
    {
      "name": "PPI Community",
      "homepage": "http://www.ppi.io/contributors"
    }
  ],
  "require": {
    "psr/log": "~1.0",
    "psr/http-message": "^0.9",
    "symfony/class-loader": "2.6.*@stable",
    "symfony/dependency-injection": "2.6.*@stable",
    "symfony/config": "2.6.*@stable",
    "symfony/console": "2.6.*@stable",
    "symfony/debug": "2.6.*@stable",
    "symfony/finder": "2.6.*@stable",
    "symfony/http-foundation": "2.6.*@stable",
    "symfony/routing": "2.6.*@stable",
    "symfony/templating": "2.6.*@stable",
    "symfony/yaml": "2.6.*@stable",
    "symfony/http-kernel": "2.6.*@stable",
    "symfony/framework-bundle": "2.6.*@stable",
    "zendframework/zend-eventmanager": "2.3.*@stable",
    "zendframework/zend-modulemanager": "2.3.*@stable",
    "zendframework/zend-servicemanager": "2.3.*@stable",
    "zendframework/zend-loader": "2.3.*@stable",
    "symfony-cmf/routing": "1.3.0",
    "aura/router": "2.2.*@stable"
  },
  "require-dev": {
    "fabpot/php-cs-fixer": "~1.5",
    "phpunit/phpunit": "~4.0",
    "satooshi/php-coveralls": "dev-master"
  },
  "autoload": {
    "psr-4": {
      "PPI\\Framework\\": "src/",
      "PPI\\FrameworkTest\\": "tests/"
    }
